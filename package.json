  "dependencies": {
    "express": "~4.16.2",
    "js-logging": "~0.1.0",
    "mongodb": "~2.2.33",
    "node-schedule": "~1.2.5"
  },
  "devDependencies": {
    "mkdirp": "0.5.1",
    "mocha": "~4.0.1",
    "rewire": "~3.0.2"
